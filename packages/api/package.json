{
  "name": "@vue/devtools-api",
  "version": "6.0.0-beta.2",
  "description": "Interact with the Vue devtools from the page",
  "main": "dist/cjs/index.js",
  "browser": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Guillaume Chau"
  },
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/vuejs/vue-devtools.git",
    "type": "git",
    "directory": "packages/api"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --module es2015 --outDir dist/es -d",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:watch": "yarn ts -w --sourceMap",
    "ts": "yarn build:esm"
  },
  "devDependencies": {
    "@types/node": "^13.9.1",
    "@types/webpack-env": "^1.15.1",
    "typescript": "^3.8.3"
  }
}
